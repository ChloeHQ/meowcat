/* pages/community/community.wxss */
.bgcolor-cd9057{
    background: #cd9057;
    padding: 36rpx;
}
.bgcolor-cd9057 .top-left{
    display: inline-block;
    width: 160rpx;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 40rpx;
    color: #d0aa86;
}
.bgcolor-cd9057 .top-center{
    color: #fff;
}
.bgcolor-cd9057 .top-icon{
    float: right;
    margin-top: 16rpx;
    width: 80rpx;
    height: 80rpx;
}
.bgcolor-cd9057 .isflex{
    display: flex;
    flex-direction:row;
    flex-wrap: nowrap;
    align-items: flex-start ;
}
.bgcolor-cd9057 .isflex .top-left{
    margin-right: 10rpx;
}
.bgcolor-cd9057 .isflex .top-left,.bgcolor-cd9057 .isflex .top-center{
    line-height: 50rpx;
}

